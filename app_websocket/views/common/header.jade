doctype 5
html
    head
        title=title
        link(rel='stylesheet', href='/bscss/bootstrap.css')
        link(rel='stylesheet', href='/bscss/bootstrap-responsive.css')
        style
            body{
                padding:60px 0 0 40px;
            }
        script
            // 下面是 seajs 的异步载入代码：
            ;(function(m, o, d, u, l, a, r) {
            if(m[d]) return
            function f(n, t) { return function() { r.push(n, arguments); return t } }
            m[d] = a = { args: (r = []), config: f(0, a), use: f(1, a) }
            m.define = f(2)
            u = o.createElement('script')
            u.id = d + 'node'
            u.src = '/sea-modules/seajs/1.3.0/sea.js'
            l = o.getElementsByTagName('head')[0]
            a = o.getElementsByTagName('base')[0]
            a ? l.insertBefore(u, a) : l.appendChild(u)
            })(window, document, 'seajs');

            // 下面立刻就可以调用 seajs 的方法了：
            seajs.config({
            alias: {
            'jquery': '/sea-modules/gallery/jquery/1.8.3/jquery',
            'bootstrap': '/bsjs/bootstrap'
            }
            });
    body
        div.navbar.navbar-inverse.navbar-fixed-top
            div.navbar-inner
                div.container
                    a.brand #{title}
                    div.nav-collapse.collapse
                      ul.nav
                        li
                          a(href="/chat.n") Chat
                        li
                          a(href="/video.n") Video 
        block content
